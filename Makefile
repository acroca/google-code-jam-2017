	mix escript.build && time ./gcj cr.2 < cr.p2.in.txt > cr.p2.out.txt
cr.p3:
	mix escript.build && time ./gcj cr.3 < cr.p3.in.txt > cr.p3.out.txt
r1.p1:
	mix escript.build && time ./gcj r1.1 < r1.p1.in.txt > r1.p1.out.txt
